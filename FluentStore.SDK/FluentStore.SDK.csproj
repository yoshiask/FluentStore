<Project Sdk="Microsoft.NET.SDK">

    <PropertyGroup>
        <TargetFramework>$(AppTargetFramework)</TargetFramework>
        <TargetPlatformMinVersion>$(MinWindowsVersion)</TargetPlatformMinVersion>
        <LangVersion>latest</LangVersion>
        <Platforms>AnyCPU;x64;x86;ARM64</Platforms>
        <Version>$(FluentStoreVersion)</Version>
        <AssemblyVersion>$(Version)</AssemblyVersion>
        <FileVersion>$(AssemblyVersion)</FileVersion>
        <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="FuseSharp" Version="1.4.0" />
        <PackageReference Include="Garfoot.Utilities.FluentUrn" Version="1.2.1" />
        <PackageReference Include="Humanizer" Version="2.14.1" />
        <PackageReference Include="IdentityModel.OidcClient" Version="5.2.1" />
        <PackageReference Include="Microsoft.PowerShell.SDK" Version="7.4.0" />
        <PackageReference Include="Microsoft.WindowsAppSDK" Version="1.3.230602002" />
        <PackageReference Include="Microsoft.Windows.SDK.BuildTools" Version="10.0.22621.2428" />
        <PackageReference Include="NuGet.PackageManagement" Version="6.8.0" />
        <PackageReference Include="NuGet.Packaging" Version="6.8.0" />
        <PackageReference Include="NuGet.Protocol" Version="6.8.0" />
        <PackageReference Include="OwlCore" Version="0.4.1" />
        <PackageReference Include="OwlCore.Kubo" Version="0.14.0" />
        <PackageReference Include="OwlCore.Storage" Version="0.9.2" />
        <PackageReference Include="PInvoke.User32" Version="0.7.124" />
        <PackageReference Include="System.Management" Version="8.0.0" />
        <PackageReference Include="System.Runtime.InteropServices.WindowsRuntime" Version="4.3.0" />
        <PackageReference Include="Vanara.PInvoke.Kernel32" Version="3.4.17" />
    </ItemGroup>

    <ItemGroup>
        <ProjectReference Include="..\FluentStore.SDK.AbstractUI\FluentStore.SDK.AbstractUI.csproj" />
        <ProjectReference Include="..\FluentStore.Services\FluentStore.Services.csproj" />
    </ItemGroup>

</Project>
